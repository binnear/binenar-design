.foot {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 60px;
  line-height: 60px;
  text-align: center;
   a {
    display: inline-block;
    text-decoration: none;
  }
  img {
    float: left;
    margin-top: 20px;
  }
  span {
    float: left;
    margin: 0 5px 0 5px;
    color: #939393;
  }
}
