.foot {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 3em;
  line-height: 3em;
  text-align: center;
   a {
    display: inline-block;
    text-decoration: none;
  }
  img {
    float: left;
    margin-top: 1em;
  }
  span {
    float: left;
    margin: 0 .4em 0 .4em;
    color: #939393;
  }
}
